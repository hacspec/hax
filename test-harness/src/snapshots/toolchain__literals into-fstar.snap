      <:
      u64) +!
    (cast (xs <: usize) <: u64)
  in
  let (_: u32):u32 =
    ((((cast (x8 <: u8) <: u32) +! (cast (x16 <: u16) <: u32) <: u32) +! x32 <: u32) +!
      (cast (x64 <: u64) <: u32)
      <:
      u32) +!
    (cast (xs <: usize) <: u32)
  in
  let (_: u16):u16 =
    ((((cast (x8 <: u8) <: u16) +! x16 <: u16) +! (cast (x32 <: u32) <: u16) <: u16) +!
