{
    "problemMatcher": [
        {
            "owner": "coq",
            "pattern": [
                {
                    "regexp": "^File \"([^\"].*)\", line (\\d+), characters (\\d+)-(\\d+):$",
                    "file": 1,
                    "line": 2,
                    "column": 3
                },
                {
                    "regexp": "^(Error|Warning): (.*)$",
                    "severity": 1,
                    "message": 2
                }
            ]
        }
    ]
}
